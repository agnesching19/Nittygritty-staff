<div class="container">
  <h1>Vote Your Favorite Staff</h1>

  <%= simple_form_for [@staff, @vote] do |f| %>
    <div class="form-inputs">
      <%= f.input :voted_staff, collection: ['Simon', 'Agnes'] %>
    </div>

    <div class="form-actions">
      <%= f.button :submit, 'VOTE', class: "btn btn-info" %>
    </div>
  <% end %>
</div>
